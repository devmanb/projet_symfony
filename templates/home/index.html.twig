{% extends 'base.html.twig' %}

{% block title %}Accueil!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
	<div class="row"><img class="img-responsive" src="{{ asset('assets/images/image.png') }}" alt=""></div>
	<hr>
	<div class="row"><h1 class="col-12" style="text-align:center;">Bienvenue dans l'application {{ app.user.username | title}}</h1></div>
</div>
{% endblock %}
